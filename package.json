{
  "name": "@47nordmedia/k2-systems-ui-library",
  "version": "0.1.3",
  "description": "K2 Systems UI library for web based projects",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "build:production": "rimraf dist && tsc --project tsconfig.json && cp ./tailwind.config.js ./dist/tailwind.config.js && npm run build:css",
    "build:tailwind": "npx tailwindcss -i ./dist/main.css -o ./dist/tailwind-compiled.css -c ./tailwind.build.config.js",
    "build:css": "ts-node --project tsconfig.json ./src/builder/index.ts --s",
    "build:js": "ts-node --project tsconfig.json ./src/builder/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/47NordMedia/k2-systems-ui-library.git"
  },
  "author": "florianporada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/47NordMedia/k2-systems-ui-library/issues"
  },
  "env": {
    "amd": true,
    "node": true,
    "commonjs": true
  },
  "homepage": "https://github.com/47NordMedia/k2-systems-ui-library#readme",
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "precss": "^4.0.0",
    "tailwindcss": "^3.0.15",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
